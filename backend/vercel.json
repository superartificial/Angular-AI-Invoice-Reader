{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.8"
    }
  },
  "env": {
    "ANGULAR_APP_URL": "https://angular-ai-invoice-reader-d7s8.vercel.app/"
  }  
}